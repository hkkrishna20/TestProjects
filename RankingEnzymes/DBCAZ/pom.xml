<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<groupId>DBCAZ</groupId>
	<artifactId>DBCAZ</artifactId>
	<packaging>jar</packaging>
	<properties>
		<java.version>1.8</java.version>
		<start-class>Application</start-class>
	</properties>
	<parent>
		<groupId>org.springframework.boot</groupId>
		<artifactId>spring-boot-starter-parent</artifactId>
		<version>2.0.3.RELEASE</version>
	</parent>
	<build>
		<sourceDirectory>src/main/java</sourceDirectory>
		<finalName>RankingCAzEnzymes</finalName>
		<plugins>
			<plugin>
				<groupId>org.jvnet.jaxb2.maven2</groupId>
				<artifactId>maven-jaxb2-plugin</artifactId>
				<version>0.13.2</version>
				<executions>
					<execution>
						<goals>
							<goal>generate</goal>
						</goals>
					</execution>
				</executions>
				<configuration>
					<generatePackage>com.RankingCAzEnzymes.schema</generatePackage>
					<generateDirectory>${basedir}/src/main/java</generateDirectory>
					<schemaDirectory>${project.basedir}/src/main/resources/xsd/</schemaDirectory>
					<schemaIncludes>
						<include>*.xsd</include>
					</schemaIncludes>
				</configuration>
			</plugin>
			<plugin>
				<artifactId>maven-dependency-plugin</artifactId>
				<executions>
					<execution>
						<phase>process-sources</phase>
						<goals>
							<goal>copy-dependencies</goal>
						</goals>
						<configuration>
							<outputDirectory>${project.build.directory}/lib</outputDirectory>
						</configuration>
					</execution>
				</executions>
			</plugin>
			<plugin>
				<groupId>org.springframework.boot</groupId>
				<artifactId>spring-boot-maven-plugin</artifactId>
				<configuration>
					<executable>true</executable>
				</configuration>
			</plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-surefire-plugin</artifactId>
				<configuration>
					<verbose>true</verbose>
					<fork>true</fork>
					<argLine>-XX:MaxPermSize=500M</argLine>
				</configuration>
			</plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-jar-plugin</artifactId>
				<configuration>
					<archive>
						<manifest>
							<addClasspath>true</addClasspath>
							<classpathPrefix>lib/</classpathPrefix>
							<mainClass>Application</mainClass>
						</manifest>
						<manifestEntries>
							<Class-Path>.</Class-Path>
						</manifestEntries>
					</archive>
					<finalName>${project.build.finalName}</finalName>
				</configuration>
			</plugin>
		</plugins>
		<resources>
			<resource>
				<directory>src/main/resources</directory>
				<includes>
					<include>**/*.properties</include>
					<include>**/*.xlsx</include>
					<include>**/*.txt</include>
					<include>**/*.jpg</include>
					<include>**/*.png</include>
					<include>**/*.jpeg</include>
					<include>**/*.gif</include>
				</includes>
			</resource>
			<resource>
				<directory>src/main/webapp/templates</directory>
				<includes>
					<include>**/*.jsp</include>
				</includes>
				<targetPath>${project.build.directory}/classes/templates</targetPath>
			</resource>
			<resource>
				<directory>src/main/webapp/templates</directory>
				<includes>
					<include>**/*.jsp</include>
				</includes>
				<targetPath>${project.build.directory}/templates</targetPath>
			</resource>
			<resource>
				<directory>src/main/resources</directory>
				<targetPath>${project.build.directory}/resources</targetPath>
			</resource>
			<resource>
				<directory>src/main/webapp/resources</directory>
				<targetPath>${project.build.directory}/resources</targetPath>
			</resource>
			<resource>
				<directory>static</directory>
				<targetPath>static</targetPath>
			</resource>
			<resource>
				<directory>lib/</directory>
			</resource>
			<resource>
				<directory>${project.build.directory}/classes/</directory>
				<targetPath>/</targetPath>
				<includes>
					<include>**/*.jar</include>
				</includes>
			</resource>
			<resource>
				<directory>${project.build.directory}/lib</directory>
				<includes>
					<include>**/*.jar</include>
				</includes>
				<targetPath>.</targetPath>
			</resource>
		</resources>
	</build>
